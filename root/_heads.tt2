<table class='heads'>
 <thead>
  <tr>
   <th>age</th>
   <th>branch</th>
   <th>actions</th>
  </tr>
 </thead>
 <tfoot>
  <tr>
   <td>age</td>
   <td>branch</td>
   <td>actions</td>
  </tr>
 </tfoot>

 <tbody>
 [% FOREACH head IN heads %]
  <tr>
   <td>[% time_since(head.last_change) %]</td>
   <td class='head[% head.sha1 == HEAD ? ' current' : '' %]'>[% head.name %]</td>
   <td class='action-list'>
     <a href="[% c.uri_for("shortlog", {h="refs/heads/" _ head.name}) %]">shortlog</a>
     <a href="[% c.uri_for("log", {h="refs/heads/" _ head.name}) %]">log</a>
     <a href="[% c.uri_for("tree", {h=head.tree_sha1, hb="refs/heads/" _ head.name}) %]">tree</a>
   </td>
  </tr>
 [% END %]
 </tbody>
</table>
